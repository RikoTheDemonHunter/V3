StarterDrink.Name = "Starter Drink"
StarterDrink.Parent = ScreenGui
StarterDrink.Style = 1
StarterDrink.BackgroundColor3 = Color3.new(0, 0, 0)
StarterDrink.Position = UDim2.new(0.219, 0, 0.365, 0)
StarterDrink.Size = UDim2.new(0, 95, 0, 21)
StarterDrink.Font = Enum.Font.GothamBlack
StarterDrink.Text = "Starter Drink"
StarterDrink.TextColor3 = Color3.new(255, 0, 0)
StarterDrink.TextSize = 14
StarterDrink.Visible = false 
StarterDrink.MouseButton1Down:connect(function()
StarterDrink.Text = "Executed"
while wait (2.34) do
local Remote = game.ReplicatedStorage.RemoteEvents['DrinkEvent']
 
local Arguments = {
        [1] = "Starter Drink"
}

Remote:FireServer(unpack(Arguments))
end
end)

